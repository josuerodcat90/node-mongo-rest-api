{
  "name": "mongo-rest-api",
  "version": "0.0.1",
  "description": "Simple Node-MongoDB REST API",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "dev": "nodemon src/index.js --exec babel-node",
    "build-babel": "babel src --out-dir dist",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node dist/index.js"
  },
  "keywords": [
    "mongodb",
    "rest",
    "api",
    "mongoose",
    "express",
    "node"
  ],
  "author": "Josue Rodriguez",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.3.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2"
  }
}